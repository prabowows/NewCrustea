{
  "rules": {
    "Device": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "Kolam": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    }
  }
}
